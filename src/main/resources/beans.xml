<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="actor" class="com.stackroute.domain.Actor">
        <property name="name" value="Salman Khan"/>
        <property name="gender" value="male"/>
        <property name="age" value="45"/>
    </bean>

    <bean id="actr2" class="com.stackroute.domain.Actor">
        <property name="name" value="Akshay Kumar"/>
        <property name="gender" value="male"/>
        <property name="age" value="50"/>
    </bean>

    <bean id="actr3" class="com.stackroute.domain.Actor">
        <property name="name" value="SRK"/>
        <property name="gender" value="male"/>
        <property name="age" value="52"/>
    </bean>

    <bean  id="movie1" class="com.stackroute.domain.Movie" autowire="byName">

    </bean>

    <bean  id="movie3" class="com.stackroute.domain.Movie">
        <constructor-arg ref="actr3"/>

    </bean>

    <bean  id="movie2" class="com.stackroute.domain.Movie" autowire="byName">
        <property name="actor" ref="actr2"/>

    </bean>





</beans>